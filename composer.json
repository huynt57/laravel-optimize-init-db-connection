{
    "name": "huy-nguyen/laravel-optimize-init-db-connection",
    "description": "This is my package laravel-optimize-init-db-connection",
    "keywords": [
        "huynt57",
        "laravel",
        "laravel-optimize-init-db-connection"
    ],
    "homepage": "https://github.com/huy-nguyen/laravel-optimize-init-db-connection",
    "license": "MIT",
    "authors": [
        {
            "name": "huynt57",
            "email": "huynt57@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.12",
        "illuminate/database": ">=8.12",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Huynt57\\LaravelOptimizeInitDbConnection\\": "src/",
            "Huynt57\\LaravelOptimizeInitDbConnection\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huynt57\\LaravelOptimizeInitDbConnection\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-optimize-init-db-connection --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huynt57\\LaravelOptimizeInitDbConnection\\LaravelOptimizeInitDbConnectionServiceProvider"
            ],
            "aliases": {
                "LaravelOptimizeInitDbConnection": "Huynt57\\LaravelOptimizeInitDbConnection\\Facades\\LaravelOptimizeInitDbConnection"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
