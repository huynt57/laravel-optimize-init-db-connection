{
    "name": "huy-nguyen/laravel-optimize-init-db-connection",
    "description": "A package for optimize Laravel init database connection",
    "keywords": [
        "huynt57",
        "laravel",
        "laravel-optimize-init-db-connection"
    ],
    "homepage": "https://github.com/huy-nguyen/laravel-optimize-init-db-connection",
    "license": "MIT",
    "authors": [
        {
            "name": "huynt57",
            "email": "huynt57@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=6.0",
        "illuminate/database": ">=6.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Huynt57\\LaravelOptimizeInitDbConnection\\": "src/",
            "Huynt57\\LaravelOptimizeInitDbConnection\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huynt57\\LaravelOptimizeInitDbConnection\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huynt57\\LaravelOptimizeInitDbConnection\\LaravelOptimizeInitDbConnectionServiceProvider"
            ],
            "aliases": {
                "LaravelOptimizeInitDbConnection": "Huynt57\\LaravelOptimizeInitDbConnection\\Facades\\LaravelOptimizeInitDbConnection"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
